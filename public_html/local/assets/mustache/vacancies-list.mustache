{{#items.isNotEmpty}}
    <div class="vacancies-list">
        {{#items}}
            <div class="vacancies-card">
                {{#title}}
                    <div class="vacancies-card__title">{{{.}}}</div>
                {{/title}}
                <div class="vacancies-card-content">
                    {{#company}}
                        <div class="vacancies-card-item vacancies-card__company">
                            <div class="vacancies-card-item__icon">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none"
                                     viewBox="0 0 20 20">
                                    <path stroke="#6A6A6A" stroke-linecap="round" stroke-linejoin="round"
                                          stroke-width="2"
                                          d="M10 1.667a8.333 8.333 0 1 0 0 16.667 8.333 8.333 0 0 0 0-16.667ZM10 13.334V10M10 6.667h-.008"/>
                                </svg>
                            </div>
                            <div class="vacancies-card-item__value">{{{.}}}</div>
                        </div>
                    {{/company}}
                    {{#experience}}
                        <div class="vacancies-card-item vacancies-card__company">
                            <div class="vacancies-card-item__icon">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none"
                                     viewBox="0 0 20 20">
                                    <path stroke="#6A6A6A" stroke-linecap="round" stroke-linejoin="round"
                                          stroke-width="2"
                                          d="M16.667 5.833H3.334c-.92 0-1.667.747-1.667 1.667v8.334c0 .92.746 1.666 1.667 1.666h13.333c.92 0 1.667-.746 1.667-1.666V7.5c0-.92-.747-1.667-1.667-1.667Z"/>
                                    <path stroke="#6A6A6A" stroke-linecap="round" stroke-linejoin="round"
                                          stroke-width="2"
                                          d="M13.334 17.5V4.167A1.667 1.667 0 0 0 11.667 2.5H8.334a1.667 1.667 0 0 0-1.667 1.667V17.5"/>
                                </svg>
                            </div>
                            <div class="vacancies-card-item__value">{{{.}}}</div>
                        </div>
                    {{/experience}}
                    {{#location}}
                        <div class="vacancies-card-item vacancies-card__company">
                            <div class="vacancies-card-item__icon">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none"
                                     viewBox="0 0 20 20">
                                    <path stroke="#6A6A6A" stroke-linecap="round" stroke-linejoin="round"
                                          stroke-width="2"
                                          d="M17.5 8.334c0 5.833-7.5 10.833-7.5 10.833s-7.5-5-7.5-10.834a7.5 7.5 0 1 1 15 0Z"/>
                                    <path stroke="#6A6A6A" stroke-linecap="round" stroke-linejoin="round"
                                          stroke-width="2" d="M10 10.834a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5Z"/>
                                </svg>
                            </div>
                            <div class="vacancies-card-item__value">{{{.}}}</div>
                        </div>
                    {{/location}}
                </div>
                {{#salary}}
                    <div class="vacancies-card-salary">
                        <div class="vacancies-card-salary__icon">
                            <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="none"
                                 viewBox="0 0 28 28">
                                <circle cx="14" cy="14" r="13" stroke="#005DE9" stroke-width="2"/>
                                <path fill="#005DE9"
                                      d="M14.79 15.62H8.693v-1.734h6.051c.489 0 .89-.077 1.205-.233.318-.155.553-.37.704-.647.156-.277.232-.6.228-.972a1.98 1.98 0 0 0-.228-.977 1.62 1.62 0 0 0-.693-.688c-.307-.17-.697-.255-1.17-.255h-2.284V20h-2.103V8.364h4.387c.901 0 1.665.163 2.29.488.625.322 1.098.76 1.42 1.313a3.56 3.56 0 0 1 .489 1.852c0 .712-.165 1.34-.495 1.88-.33.539-.806.961-1.431 1.268-.625.303-1.383.454-2.273.454Zm.267.863v1.733H8.693v-1.733h6.364Z"/>
                            </svg>
                        </div>
                        <div class="vacancies-card-salary__value">{{{.}}}</div>
                    </div>
                {{/salary}}
                {{#href}}
                    <a href="{{.}}" class="link-as-card"></a>
                {{/href}}
            </div>
        {{/items}}
    </div>
{{/items.isNotEmpty}}